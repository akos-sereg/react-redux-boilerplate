{
  "name": "react-redux-ts-boilerplate",
  "version": "1.0.0",
  "description": "A minimal react-redux boilerplate with all the best practices",
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack --config config/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "start": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider node server",
    "clean": "npm run test:clean && npm run build:clean",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "pretest": "npm run test:clean",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^3.4.1",
    "chalk": "^2.3.2",
    "compression": "1.7.2",
    "cross-env": "^5.1.6",
    "express": "^4.18.2",
    "fontfaceobserver": "2.0.13",
    "history": "4.7.2",
    "hoist-non-react-statics": "2.5.0",
    "immutable": "3.8.2",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "react-router-dom": "5.3.4",
    "redux": "4.2.1",
    "redux-immutable": "4.0.0",
    "redux-saga": "0.16.2",
    "reselect": "3.0.1",
    "sanitize.css": "4.1.0",
    "toastr": "^2.1.4",
    "typesafe-actions": "^5.1.0",
    "warning": "^4.0.1",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@cfaester/enzyme-adapter-react-18": "^0.7.1",
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.181",
    "@types/react": "^17.0.43",
    "@types/react-redux": "^7.1.23",
    "@types/react-router-dom": "^5.3.3",
    "@types/toastr": "^2.1.39",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "add-asset-html-webpack-plugin": "2.1.3",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "^29.5.0",
    "babel-loader": "7.1.4",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "circular-dependency-plugin": "5.0.2",
    "css-loader": "0.28.11",
    "cypress": "^12.11.0",
    "enzyme": "^3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-redux-saga": "^0.9.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "^4.3.1",
    "imports-loader": "0.8.0",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "jest-css-modules-transform": "^4.4.2",
    "lint-staged": "^7.1.3",
    "node-plop": "^0.15.0",
    "node-sass": "9.0.0",
    "null-loader": "0.1.1",
    "plop": "2.0.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "^18.2.0",
    "redux-devtools-extension": "^2.13.9",
    "rimraf": "2.6.2",
    "sass-loader": "^7.0.1",
    "shelljs": "^0.8.5",
    "style-loader": "^0.21.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^8.2.0",
    "typescript": "5.0.4",
    "url-loader": "1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.25.3"
  }
}
